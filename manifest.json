{
  "manifest_version": 2,

  "name": "ZeAlAnglit",
  "description": "You know that moment when you realize you're keyboard is on English?",
  "version": "1.0.0",

  "browser_action": {
   "default_icon": "logo.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "clipboardWrite",
   "clipboardRead"
   ],
   "content_scripts": [
	{
	"matches": ["http://*/*"],
	"css": ["background.css"]
	} 
   ]
}
